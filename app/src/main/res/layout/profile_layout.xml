<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <ImageView
        android:id="@+id/imageView14"
        android:layout_width="384dp"
        android:layout_height="191dp"
        android:layout_marginBottom="16dp"
        android:layout_marginTop="8dp"
        app:layout_constraintBottom_toTopOf="@+id/b_logout"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/b_editprof"
        app:layout_constraintVertical_bias="0.444"
        app:srcCompat="@color/colorPrimaryDark" />

    <ImageView
        android:id="@+id/imageView12"
        android:layout_width="114dp"
        android:layout_height="100dp"
        android:layout_marginBottom="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="16dp"
        app:layout_constraintBottom_toTopOf="@+id/tableLayout3"
        app:layout_constraintEnd_toStartOf="@+id/textView21"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/ic_action_user" />

    <TextView
        android:id="@+id/textView21"
        android:layout_width="wrap_content"
        android:layout_height="26dp"
        android:layout_marginBottom="12dp"
        android:layout_marginEnd="104dp"
        android:layout_marginTop="8dp"
        android:text="Mharjorie Sandel"
        android:textColor="@android:color/black"
        android:textSize="18sp"
        app:layout_constraintBottom_toTopOf="@+id/b_editprof"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0" />

    <Button
        android:id="@+id/b_editprof"

        android:layout_width="130dp"
        android:layout_height="29dp"

        android:layout_marginBottom="44dp"
        android:background="@drawable/button"
        android:text="EDIT PROFILE"
        android:textColor="#8d0000"
        android:textSize="15sp"
        app:layout_constraintBottom_toTopOf="@+id/tableLayout3"
        app:layout_constraintStart_toEndOf="@+id/imageView12" />

    <Button
        android:id="@+id/b_logout"

        android:layout_width="130dp"
        android:layout_height="29dp"

        android:layout_marginBottom="88dp"
        android:layout_marginEnd="8dp"
        android:layout_marginStart="16dp"
        android:background="@drawable/button"
        android:text="LOG OUT"
        android:textColor="#8d0000"
        android:textSize="15sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.482"
        app:layout_constraintStart_toStartOf="parent" />

    <TableLayout xmlns:android="http://schemas.android.com/apk/res/android"
        android:id="@+id/tableLayout3"
        android:layout_width="fill_parent"
        android:layout_height="189dp"
        android:layout_marginBottom="68dp"
        android:stretchColumns="1"
        app:layout_constraintBottom_toTopOf="@+id/b_logout"
        tools:layout_editor_absoluteX="0dp">


        <TableRow>
            <!-- Column 1 -->
            <TextView
                android:id="@+id/tbl_txt5"
                android:layout_width="100dp"
                android:layout_height="wrap_content"
                android:layout_margin="4dip"
                android:layout_weight="1"
                android:gravity="left"
                android:padding="10dip"
                android:text="Height (cm)"
                android:textColor="@android:color/black" />

            <!-- Column 2 -->
            <TextView
                android:id="@+id/tbl_txt6"
                android:layout_width="0dip"
                android:layout_height="wrap_content"
                android:layout_margin="4dip"
                android:layout_weight="1"
                android:gravity="right"
                android:padding="10dip"
                android:text="145"
                android:textColor="@color/colorAccent"
                android:textColorHighlight="@color/colorAccent"
                android:textColorHint="@color/colorAccent" />

            <!-- Column 3 -->

        </TableRow>

        <View
            android:layout_height="0.5dip"
            android:background="#FF909090" />

        <TableRow>
            <!-- Column 1 -->
            <TextView
                android:id="@+id/tbl_txt7"
                android:layout_width="100dp"
                android:layout_height="wrap_content"
                android:layout_margin="4dip"
                android:layout_weight="1"
                android:gravity="left"
                android:padding="10dip"
                android:text="Weight (lbs)"
                android:textColor="@android:color/background_dark" />

            <!-- Column 2 -->
            <TextView
                android:id="@+id/tbl_txt8"
                android:layout_width="0dip"
                android:layout_height="wrap_content"
                android:layout_margin="4dip"
                android:layout_weight="1"
                android:gravity="right"
                android:padding="10dip"
                android:text="136"
                android:textColor="@color/colorAccent" />

            <!-- Column 3 -->

        </TableRow>

        <View
            android:layout_height="0.5dip"
            android:background="#FF909090" />

        <TableRow>
            <!-- Column 1 -->
            <TextView
                android:id="@+id/tbl_txt3"
                android:layout_width="100dp"
                android:layout_height="wrap_content"
                android:layout_margin="4dip"
                android:layout_weight="1"
                android:gravity="left"
                android:padding="10dip"
                android:text="Min Blood Sugar Level (mg/dL)"
                android:textColor="@android:color/background_dark" />

            <!-- Column 2 -->
            <TextView
                android:id="@+id/tbl_txt4"
                android:layout_width="0dip"
                android:layout_height="wrap_content"
                android:layout_margin="4dip"
                android:layout_weight="1"
                android:gravity="right"
                android:padding="10dip"
                android:text="20"
                android:textColor="@color/colorAccent" />

            <!-- Column 3 -->

        </TableRow>

        <View
            android:layout_height="0.5dip"
            android:background="#FF909090" />

        <TableRow>
            <!-- Column 1 -->
            <TextView
                android:id="@+id/tbl_txt1"
                android:layout_width="100dp"
                android:layout_height="wrap_content"
                android:layout_margin="4dip"
                android:layout_weight="1"
                android:gravity="left"
                android:padding="10dip"
                android:text="Max Blood Sugar Level (mg/dL)"
                android:textColor="@android:color/background_dark" />

            <!-- Column 2 -->
            <TextView
                android:id="@+id/tbl_txt2"
                android:layout_width="0dip"
                android:layout_height="wrap_content"
                android:layout_margin="4dip"
                android:layout_weight="1"
                android:gravity="right"
                android:padding="10dip"
                android:text="06-17-1999"
                android:textColor="@color/colorAccent" />

            <!-- Column 3 -->

        </TableRow>


    </TableLayout>

</android.support.constraint.ConstraintLayout>